var a;
var b;

a = 4;
b = 10;

if (b / a == 2) {
    print "b / a == 2";
}else{
    print "b / a != 2";
}

if ( a == 5 and b == 10) {
    print "a == 5 and b == 10";
}else {
    if (a == 4 and b == 10) {
        print "a == 4 and b == 10";
    } else {
        print "fu";
    }
    print "a != 5 or b != 10";
}
